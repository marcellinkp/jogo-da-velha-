    @Sobrepor
    protected  void  onCreate ( Bundle  savedInstanceState ) {
        super . onCreate (savedInstanceState);
        setContentView ( R . disposição . activity_principal);
    }

    public  void  newGame ( Exibir  visualização ) {
        Intenção callDisplay =  new  Intent ( este , Jocks . Classe);
        startActivity (callDisplay);
    }

    public  void  encerra ( Exibir  visualização ) {
        Intent encerra =  novo  Intent ( Intent . ACTION_MAIN );
